{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "Public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/stylesLogin.css",
      "dest": "/Public/stylesLogin.css"
    },
    {
      "src": "/img/(.*)",
      "dest": "/Public/img/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
